{
  "name": "ergebnis/environment-variables",
  "description": "Provides an abstraction for environment variables.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/environment-variables",
  "support": {
    "issues": "https://github.com/ergebnis/environment-variables/issues",
    "source": "https://github.com/ergebnis/environment-variables"
  },
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.25.2",
    "ergebnis/data-provider": "^1.2.0",
    "ergebnis/license": "^1.2.0",
    "ergebnis/php-cs-fixer-config": "~4.4.0",
    "infection/infection": "~0.26.6",
    "phpunit/phpunit": "^9.5.20",
    "psalm/plugin-phpunit": "~0.16.1",
    "vimeo/psalm": "^4.23"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Environment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Environment\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "7.4.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
