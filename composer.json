{
  "name": "ergebnis/environment-variables",
  "type": "library",
  "description": "Provides an abstraction for environment variables.",
  "homepage": "https://github.com/ergebnis/environment-variables",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.15.0",
    "ergebnis/license": "^1.1.0",
    "ergebnis/php-cs-fixer-config": "^2.14.0",
    "ergebnis/phpstan-rules": "~0.15.3",
    "ergebnis/test-util": "^1.5.0",
    "infection/infection": "~0.15.3",
    "jangregor/phpstan-prophecy": "~0.8.1",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "~0.12.98",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.22",
    "phpstan/phpstan-strict-rules": "~0.12.11",
    "phpunit/phpunit": "^8.5.20",
    "psalm/plugin-phpunit": "~0.16.1",
    "vimeo/psalm": "^4.9.3"
  },
  "config": {
    "platform": {
      "php": "7.2.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Environment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Environment\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/environment-variables/issues",
    "source": "https://github.com/ergebnis/environment-variables"
  }
}
