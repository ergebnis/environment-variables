{
  "name": "ergebnis/environment-variables",
  "description": "Provides an abstraction for environment variables.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/environment-variables",
  "support": {
    "issues": "https://github.com/ergebnis/environment-variables/issues",
    "source": "https://github.com/ergebnis/environment-variables",
    "security": "https://github.com/ergebnis/environment-variables/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0 || ~8.5.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.48.2",
    "ergebnis/data-provider": "^3.6.0",
    "ergebnis/license": "^2.7.0",
    "ergebnis/php-cs-fixer-config": "^6.59.0",
    "ergebnis/phpunit-slow-test-detector": "^2.20.0",
    "infection/infection": "~0.27.11",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^1.12.10",
    "phpstan/phpstan-deprecation-rules": "^1.2.1",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpstan/phpstan-strict-rules": "^1.6.1",
    "phpunit/phpunit": "^10.5.62",
    "rector/rector": "^1.2.10",
    "roave/backward-compatibility-check": "^8.6.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Environment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Environment\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.6-dev"
    },
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
