{
  "name": "ergebnis/environment-variables",
  "description": "Provides an abstraction for environment variables.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/environment-variables",
  "support": {
    "issues": "https://github.com/ergebnis/environment-variables/issues",
    "source": "https://github.com/ergebnis/environment-variables"
  },
  "require": {
    "php": "~8.0.0 || ~8.1.0 || ~8.2.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.29.0",
    "ergebnis/data-provider": "^1.3.0",
    "ergebnis/license": "^2.1.0",
    "ergebnis/php-cs-fixer-config": "~5.3.1",
    "infection/infection": "~0.26.16",
    "phpunit/phpunit": "^9.6.3",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.15.13",
    "vimeo/psalm": "^5.6.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Environment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Environment\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.0.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
