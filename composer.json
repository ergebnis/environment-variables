{
  "name": "ergebnis/environment-variables",
  "description": "Provides an abstraction for environment variables.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/environment-variables",
  "support": {
    "issues": "https://github.com/ergebnis/environment-variables/issues",
    "source": "https://github.com/ergebnis/environment-variables",
    "security": "https://github.com/ergebnis/environment-variables/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.37.0",
    "ergebnis/data-provider": "^3.0.0",
    "ergebnis/license": "^2.2.0",
    "ergebnis/php-cs-fixer-config": "^6.6.0",
    "ergebnis/phpunit-slow-test-detector": "^2.3.0",
    "infection/infection": "~0.27.3",
    "phpunit/phpunit": "^10.4.0",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.18.5",
    "vimeo/psalm": "^5.15.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Environment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Environment\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.20"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
